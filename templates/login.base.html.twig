<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% if app.request.server.get('DEPLOY_TIMESTAMP') is defined and app.request.server.get('DEPLOY_TIMESTAMP') != '{DEPLOY_TIMESTAMP}' %}
        {% set asset_version = '?version=' ~ app.request.server.get('DEPLOY_TIMESTAMP') %}
    {% else %}
        {% set asset_version = '' %}
    {% endif %}

    {% block stylesheets %}
        <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,300;0,400;0,500;0,600;1,400;1,500;1,600" rel="stylesheet">

        <!-- BEGIN: Vendor CSS-->
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/vendors/css/vendors.min.css' ~ asset_version ) }}">
        <!-- END: Vendor CSS-->

        <!-- BEGIN: Theme CSS-->
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/bootstrap.css' ~ asset_version ) }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/bootstrap-extended.css' ~ asset_version ) }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/colors.css' ~ asset_version ) }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/components.css' ~ asset_version ) }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/themes/dark-layout.css' ~ asset_version ) }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/themes/bordered-layout.css' ~ asset_version ) }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/themes/semi-dark-layout.css' ~ asset_version ) }}">

        <!-- BEGIN: Page CSS-->
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/core/menu/menu-types/vertical-menu.css' ~ asset_version ) }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets//app-assets/css/plugins/forms/form-validation.css' ~ asset_version ) }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('assets//app-assets/css/pages/authentication.css' ~ asset_version ) }}">

        <!-- END: Page CSS-->

        <!-- BEGIN: Custom CSS
    <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/custom.css' ~ asset_version ) }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('assets/app-assets/css/style.css' ~ asset_version ) }}">
    -->
        <link rel="icon" href="{{ asset('assets/app/img/conuti-logo-favicon.png' ~ asset_version ) }}" type="image/png"
              sizes="16x16"/>
    {% endblock %}
</head>
<body class="vertical-layout vertical-menu-modern  navbar-floating footer-static  " data-open="click" data-menu="vertical-menu-modern" data-col="">
{% block body %}{% endblock %}

{% block javascripts %}
