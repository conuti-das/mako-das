    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <!-- BEGIN: Vendor JS-->
    <script src="{{ asset('assets/app-assets/vendors/js/vendors.min.js' ~ asset_version ) }}"></script>
    <!-- BEGIN Vendor JS-->

    <!-- BEGIN: Page Vendor JS-->
    <!-- END: Page Vendor JS-->

    <!-- BEGIN: Theme JS-->
    <script src="{{ asset('assets/app-assets/js/core/app-menu.js' ~ asset_version ) }}"></script>
    <script src="{{ asset('assets/app-assets/js/core/app.js' ~ asset_version ) }}"></script>
    <!-- END: Theme JS-->

    <!-- BEGIN: Page Vendor JS-->
    <script src="{{ asset('assets/app-assets/vendors/js/forms/validation/jquery.validate.min.js' ~ asset_version ) }}"></script>
    <!-- END: Page Vendor JS-->

    <script>
        $(window).on('load', function() {
            if (feather) {
                feather.replace({
                    width: 14,
                    height: 14
                });
            }
        })
    </script>

{% endblock %}

</body>
</html>
